<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.transinfo.cacis.dto.accounting">

	<class name="CurrPaySumDto" table="CURR_PAY_SUM">

		<id name="cardNumber" type="long">
			<column name="CARDNO" sql-type="number" not-null="true" />
		</id>

		<property name="paySum" type="double">
			<column name="CURRPAY" sql-type="number" not-null="true" />
		</property>

	</class>
</hibernate-mapping>