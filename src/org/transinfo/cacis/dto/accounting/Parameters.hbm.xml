<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package ="org.transinfo.cacis.dto.accounting">

<class 	name="ParametersDto" table="PARAMETERS"> 

 <id name="name" type ="string" >
  <column name ="NAME" sql-type ="varchar2(50)" not-null = "true"/>
 </id>
 <property	name="value"  type ="string">
        <column name = "VALUE"  sql-type ="varchar2(50)"  not-null = "true" />
</property>

<property	name="issuerId"  type ="string">
        <column name = "ISSUER_ID"  sql-type ="varchar2(32)"  not-null = "true" />
</property>

</class>
</hibernate-mapping>