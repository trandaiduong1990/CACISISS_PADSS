<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.transinfo.cacis.dto.cardproduction">

	<class name="ApplicationTypeDto" table="APPLICATIONTYPE">
		<id name="applicationTypeId" type="integer">
		    <column name="APPLICATIONTYPEID" sql-type ="number" not-null="true" />
			<generator class="assigned" />
		</id>

		<property name="description" type="string">
			<column name="DESCRIPTION" sql-type="varchar2(30)" not-null="true" />
		</property>
		
	</class>
</hibernate-mapping>
