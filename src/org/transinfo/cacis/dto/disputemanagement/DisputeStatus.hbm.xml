<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.transinfo.cacis.dto.disputemanagement">

	<class name="DisputeStatusDto" table="DISPUTE_STATUS">
		<id name="id" type="string">
		    <column name="ID" sql-type ="varchar2(1)" not-null="true" />
			<generator class="assigned" />
		</id>

		<property name="description" type="string">
			<column name="DESCRIPTION" sql-type="varchar2(50)" not-null="true" />
		</property>

	</class>

</hibernate-mapping>