<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.transinfo.cacis.dto.common">

<class 	name="CardHolderTypeDto" table="CARDHOLDER_TYPE"> 

 <id name="cardHolderTypeId" type ="int"  unsaved-value="0">
    <column name ="TYPE_ID" sql-type ="number" not-null = "true"/>
       <generator class="assigned"/>
   </id>
     <property	name="description"  type ="string">
        <column name = "DESCRIPTION"  sql-type ="varchar2(25)" not-null = "true"/>
    </property>
   
</class>
</hibernate-mapping>