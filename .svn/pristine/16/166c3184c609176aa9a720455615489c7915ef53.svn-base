<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.transinfo.cacis.dto.accounting">

<class 	name="Interest" table="INTEREST"> 

	<id name="intId" type ="string"  unsaved-value="0">
	    <column name ="INTERSET_ID" sql-type ="varchar2(3)" not-null = "true"/>
	    <generator class="assigned"/>
	</id>

	<property name="intDesc" type="string">
		<column name="INTEREST_DESC" sql-type="varchar2(30)" not-null="false" />
	</property>
		 
	</class>
</hibernate-mapping>