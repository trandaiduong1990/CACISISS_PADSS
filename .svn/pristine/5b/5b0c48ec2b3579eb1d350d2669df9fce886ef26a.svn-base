function productLimitFormSub(frmName){
	
	if(!onSubmitValidation(frmName.maxCreditAmtPerTrnx, 8, 2)){ return false; }
	if(!onSubmitValidation(frmName.maxCashLimitPerTrnx, 8, 2)){ return false; }
	if(!onSubmitValidation(frmName.maxCreditLimitPerDay, 8, 2)){ return false; }
	if(!onSubmitValidation(frmName.maxCashLimitPerDay, 8, 2)){ return false; }
	
	return true;
	
}