<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.transinfo.cacis.dto.disputemanagement">

	<class name="Base2DocIndicatorDto" table="BASEII_DOC_INDICATOR">
		<id name="docIndicator" type="string">
		    <column name="DOC_INDICATOR" sql-type ="varchar2(1)" not-null="true" />
			<generator class="assigned" />
		</id>
	
		<property name="description" type="string">
			<column name="DESCRIPTION" sql-type="varchar2(100)" not-null="false" />
		</property>

	</class>

</hibernate-mapping>