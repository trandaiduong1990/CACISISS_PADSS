package org.transinfo.cacis.dto.accounting;

import java.io.Serializable;
import java.util.Date;

@SuppressWarnings("serial")
public class CustomerPayment implements Serializable {
	
	private String paymentId;
	private Long cardNo;
	private String accountId;
	private String payMode;
	private Double payAmt;
	private String paySrc;
	private String fullpay;
	private Date payDate;
	private Double payAdjAmt;
	private String payRefNo;
	private String amtSign;
	private String billed;
	private String updateBy;
	private Date updateDate;
	
	public String getPaymentId() {
		return paymentId;
	}
	public void setPaymentId(String paymentId) {
		this.paymentId = paymentId;
	}
	public Long getCardNo() {
		return cardNo;
	}
	public void setCardNo(Long cardNo) {
		this.cardNo = cardNo;
	}
	public String getAccountId() {
		return accountId;
	}
	public void setAccountId(String accountId) {
		this.accountId = accountId;
	}
	public String getPayMode() {
		return payMode;
	}
	public void setPayMode(String payMode) {
		this.payMode = payMode;
	}
	public Double getPayAmt() {
		return payAmt;
	}
	public void setPayAmt(Double payAmt) {
		this.payAmt = payAmt;
	}
	public String getPaySrc() {
		return paySrc;
	}
	public void setPaySrc(String paySrc) {
		this.paySrc = paySrc;
	}
	public String getFullpay() {
		return fullpay;
	}
	public void setFullpay(String fullpay) {
		this.fullpay = fullpay;
	}
	public Date getPayDate() {
		return payDate;
	}
	public void setPayDate(Date payDate) {
		this.payDate = payDate;
	}
	public Double getPayAdjAmt() {
		return payAdjAmt;
	}
	public void setPayAdjAmt(Double payAdjAmt) {
		this.payAdjAmt = payAdjAmt;
	}
	public String getPayRefNo() {
		return payRefNo;
	}
	public void setPayRefNo(String payRefNo) {
		this.payRefNo = payRefNo;
	}
	public String getAmtSign() {
		return amtSign;
	}
	public void setAmtSign(String amtSign) {
		this.amtSign = amtSign;
	}
	public String getBilled() {
		return billed;
	}
	public void setBilled(String billed) {
		this.billed = billed;
	}
	public String getUpdateBy() {
		return updateBy;
	}
	public void setUpdateBy(String updateBy) {
		this.updateBy = updateBy;
	}
	public Date getUpdateDate() {
		return updateDate;
	}
	public void setUpdateDate(Date updateDate) {
		this.updateDate = updateDate;
	}

}
