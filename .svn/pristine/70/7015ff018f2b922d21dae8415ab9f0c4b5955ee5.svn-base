package org.transinfo.cacis.formbean.disputemanagement;

import org.apache.struts.validator.ValidatorForm;
import org.transinfo.cacis.formbean.common.DateForm;

public class DisputeConfigForm extends ValidatorForm {

	private String minDisputeAmt;

	private String financeCharge;

	private char issueTempDispatchChargeAmt;

	private String transactionValidationPeriod;

	private String copyReqProcessingDays;
	
	private String chargebackProcessingDays;

	DateForm lastUpdatedDateForm = new DateForm();

	private String lastUpdatedBy;

	private String issuerId;

	public void getPreListData() {
		try {
		} catch (Exception e) {
			System.out
					.println("Error while getting WriteOffForm formbean PreListData:"
							+ e);
		}
	}

	public String getChargebackProcessingDays() {
		return chargebackProcessingDays;
	}

	public void setChargebackProcessingDays(String chargebackProcessingDays) {
		this.chargebackProcessingDays = chargebackProcessingDays;
	}

	public String getCopyReqProcessingDays() {
		return copyReqProcessingDays;
	}

	public void setCopyReqProcessingDays(String copyReqProcessingDays) {
		this.copyReqProcessingDays = copyReqProcessingDays;
