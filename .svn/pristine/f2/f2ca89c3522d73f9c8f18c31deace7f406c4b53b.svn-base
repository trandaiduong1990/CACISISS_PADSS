<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.transinfo.cacis.dto.accounting">

<class 	name="Fee" table="FEE"> 

	<id name="feeId" type ="string"  unsaved-value="0">
	    <column name ="FEE_ID" sql-type ="varchar2(3)" not-null = "true"/>
	    <generator class="assigned"/>
	</id>

	<property name="feeDesc" type="string">
		<column name="FDESC" sql-type="varchar2(30)" not-null="false" />
	</property>
		 
	</class>
</hibernate-mapping>