package org.transinfo.cacis.dto.authorization;

import java.io.Serializable;
import java.util.Date;

@SuppressWarnings("serial")
public class SystemParamDto implements Serializable {

	private String issuerId;
	private String mailServerAddress;
	private int mailServerPort;
	private String fromName;
	private String fromMail;
	private int voicePort;
	private int serverPort;
	private int dataTimeout;
	private int noWorkerThread;
	private int debugLevel;
	private Date updatedDate = new Date();
	private String userId;
	private Integer renewalTimeInterval;
	private Integer changeCardCloseInterval;
	private Integer cardClose;
	private Integer expireDateExtend;

	public SystemParamDto() {
	}

	public String getIssuerId() {
		return issuerId;
	}

	public void setIssuerId(String issuerId) {
		this.issuerId = issuerId;
	}

	public String getMailServerAddress() {
		return mailServerAddress;
	}

	public void setMailServerAddress(String mailServerAddress) {
		this.mailServerAddress = mailServerAddress;
	}

	public int getMailServerPort() {
		return mailServerPort;
	}

	public void setMailServerPort(int mailServerPort) {
		this.mailServerPort = mailServerPort;
	}

	public String getFromName() {
		return fromName;
	}

	public void setFromName(String fromName) {
		this.fromName = fromName;
	}

	public String getFromMail() {
		return fromMail;
	}

	public void setFromMail(String fromMail) {
		this.fromMail = fromMail;
	}

	public int getVoicePort() {
		return voicePort;
	}

	public void setVoicePort(int voicePort) {
		this.voicePort = voicePort;
	}

	public int getServerPort() {
		return serverPort;
	}

	public void setServerPort(int serverPort) {
		this.serverPort = serverPort;
	}

	public int getDataTimeout() {
		return dataTimeout;
	}

	public void setDataTimeout(int dataTimeout) {
		this.dataTimeout = dataTimeout;
	}

	public int getNoWorkerThread() {
		return noWorkerThread;
	}

	public void setNoWorkerThread(int noWorkerThread) {
		this.noWorkerThread = noWorkerThread;
	}

	public int getDebugLevel() {
		return debugLevel;
	}

	public void setDebugLevel(int debugLevel) {
		this.debugLevel = debugLevel;
	}

	public Date getUpdatedDate() {
		return updatedDate;
	}

	public void setUpdatedDate(Date updatedDate) {
		this.updatedDate = updatedDate;
	}

	public String getUserId() {
		return userId;
	}

	public void setUserId(String userId) {
		this.userId = userId;
	}

	public Integer getRenewalTimeInterval() {
		return renewalTimeInterval;
	}

	public void setRenewalTimeInterval(Integer renewalTimeInterval) {
		this.renewalTimeInterval = renewalTimeInterval;
	}

	public Integer getChangeCardCloseInterval() {
		return changeCardCloseInterval;
	}

	public void setChangeCardCloseInterval(Integer changeCardCloseInterval) {
		this.changeCardCloseInterval = changeCardCloseInterval;
	}

	public Integer getCardClose() {
		return cardClose;
	}

	public void setCardClose(Integer cardClose) {
		this.cardClose = cardClose;
	}

	public Integer getExpireDateExtend() {
		return expireDateExtend;
	}

	public void setExpireDateExtend(Integer expireDateExtend) {
		this.expireDateExtend = expireDateExtend;
	}
}
