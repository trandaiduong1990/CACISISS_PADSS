	public void getPreListData() {
		
		try {
			
			ApplValidationDAO objApplValidationDAO = DAOFactory.getInstance()
					.getApplValidationDAO();
			
			if (criteriaList == null) {
				criteriaList = objApplValidationDAO.getCodeMasterListByGroup("APPLCRITERIAGRP");
			}
			
			if (operatorList == null) {
				operatorList = objApplValidationDAO.getCodeMasterListByGroup("OPERATORGRP");
			}
			
			if (refFailList == null) {
				refFailList = objApplValidationDAO.getCodeMasterListByGroup("APPLACTIONGRP");
			}
			
			if (countryList == null) {
				countryList = objApplValidationDAO.countryListData();
			}
			
		} catch (Exception e) {
			System.out
			.println("Error while getting PreListData in ApplValidationProcessForm:"
					+ e);
		}
	}
