<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.transinfo.cacis.dto.disputemanagement">

	<class name="DisputeReasonDto"
		table="DISPUTE_REASON">
		<id name="code" type="int">
		     <column name="CODE" sql-type ="NUMBER (2)"  not-null = "true" />
			<generator class="assigned" />
		</id>

		<property name="description" type ="string">
			<column name="DESCRIPTION" sql-type ="VARCHAR2 (35)"  not-null = "false" />
		</property>

	</class>

</hibernate-mapping>