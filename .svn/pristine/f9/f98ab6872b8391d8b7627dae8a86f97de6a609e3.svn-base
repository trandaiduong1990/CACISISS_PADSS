package org.transinfo.cacis.dto.accounting;

import java.io.Serializable;

public class CardBillingDto implements Serializable {
	
	private static final long serialVersionUID = 1L;
	private String issuerId;
	//private String userId;
	//private Date updatedDate;
    private String billingCycleDone;
	private int prevCycleNo;
	private int prevCycleDate;
	private int nextCycleNo;
	private int nextCycleDate;
	private int numberOfAccounts;
	
	public CardBillingDto(){}

	public String getIssuerId() {
		return issuerId;
	}

	public void setIssuerId(String issuerId) {
		this.issuerId = issuerId;
	}

	public int getNextCycleDate() {
		return nextCycleDate;
	}

	public void setNextCycleDate(int nextCycleDate) {
		this.nextCycleDate = nextCycleDate;
	}

	public int getNextCycleNo() {
		return nextCycleNo;
	}

	public void setNextCycleNo(int nextCycleNo) {
		this.nextCycleNo = nextCycleNo;
	}

	public int getNumberOfAccounts() {
		return numberOfAccounts;
	}

	public void setNumberOfAccounts(int numberOfAccounts) {
		this.numberOfAccounts = numberOfAccounts;
	}

	public int getPrevCycleDate() {
		return prevCycleDate;
	}

	public void setPrevCycleDate(int prevCycleDate) {
		this.prevCycleDate = prevCycleDate;
	}

	public int getPrevCycleNo() {
		return prevCycleNo;
	}

	public void setPrevCycleNo(int prevCycleNo) {
		this.prevCycleNo = prevCycleNo;
	}

	public String getBillingCycleDone() {
		return billingCycleDone;
	}

	public void setBillingCycleDone(String billingCycleDone) {
		this.billingCycleDone = billingCycleDone;
	}

}
