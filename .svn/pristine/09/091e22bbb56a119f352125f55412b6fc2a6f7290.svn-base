package org.transinfo.cacis.dto.applicationforms;

import java.io.Serializable;
import java.util.Date;

@SuppressWarnings("serial")
public class CardsRenewalDto implements Serializable {

	private String cardRenewalSerial;
	private long cardNumber;
	private String issuerId;
	private String customerId;
	private String cardExpireDate;
	private String userId;
	private Date updatedDate = new Date();
	private int status;

	private String branchId;

	public String getCardRenewalSerial() {
		return cardRenewalSerial;
	}

	public void setCardRenewalSerial(String cardRenewalSerial) {
		this.cardRenewalSerial = cardRenewalSerial;
	}

	public long getCardNumber() {
		return cardNumber;
	}

	public void setCardNumber(long cardNumber) {
		this.cardNumber = cardNumber;
	}

	public String getIssuerId() {
		return issuerId;
	}

	public void setIssuerId(String issuerId) {
		this.issuerId = issuerId;
	}

	public String getCustomerId() {
		return customerId;
	}

	public void setCustomerId(String customerId) {
		this.customerId = customerId;
	}

	public String getCardExpireDate() {
		return cardExpireDate;
	}

	public void setCardExpireDate(String cardExpireDate) {
		this.cardExpireDate = cardExpireDate;
	}

	public String getUserId() {
		return userId;
	}

	public void setUserId(String userId) {
		this.userId = userId;
	}

	public Date getUpdatedDate() {
		return updatedDate;
	}

	public void setUpdatedDate(Date updatedDate) {
		this.updatedDate = updatedDate;
	}

	public int getStatus() {
		return status;
	}

	public void setStatus(int status) {
		this.status = status;
	}

	public String getBranchId() {
		return branchId;
	}

	public void setBranchId(String branchId) {
		this.branchId = branchId;
	}

}
