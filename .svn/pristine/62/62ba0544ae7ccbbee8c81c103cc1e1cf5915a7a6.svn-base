<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.transinfo.cacis.dto.settings">

	<class name="TranxTypeDto" table="TRANX_TYPE">
		<id name="tranxType" column="TRANX_TYPE"/>
		<property name="description" type="string">
			<column name="DESCRIPTION" sql-type="varchar2(30)" not-null="true" />
		</property>
		<property name="tranxGroup" type="string">
			<column name="TRANX_GROUP" sql-type="varchar2(3)" not-null="false" />
		</property>

	</class>
</hibernate-mapping>