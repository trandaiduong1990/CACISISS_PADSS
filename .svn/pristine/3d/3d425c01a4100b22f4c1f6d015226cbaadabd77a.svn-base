<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.transinfo.cacis.dto.disputemanagement">

	<class name="DisputeClaimTypesDto" table="DISPUTE_CLAIM_TYPE">
		<id name="claimTypeId" type="string">
		     <column name="CLAIM_TYPE" sql-type ="Char(1)"  not-null = "true" />
			<generator class="assigned" />
		</id>

		<property name="claimType" type ="string">
			<column name="CLAIM_TYPE_DESCRIPTION" sql-type ="VARCHAR2 (30)"  not-null = "false" />
		</property>

	</class>

</hibernate-mapping>