<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping  package ="org.transinfo.cacis.dto.switching">

  <class name="SwitchTypeDto" table="SWITCH_TYPE">	   
      
       <id name="switchType" type ="string">
		     <column name ="SW_TYPE" sql-type ="char(1)" not-null = "true"/>
			 <generator class="assigned"/>
       </id>
      	  	   	  	
	<property	name="switchDesc"  type ="string">
			<column name = "SW_DESC"  sql-type ="varchar2(20)"  not-null = "true"/>
		</property>
        		         
  </class>

</hibernate-mapping>