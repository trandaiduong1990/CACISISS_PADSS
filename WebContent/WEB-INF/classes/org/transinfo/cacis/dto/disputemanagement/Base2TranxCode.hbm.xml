<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.transinfo.cacis.dto.disputemanagement">

	<class name="Base2TranxCodeDto" table="BASEII_TRANXCODE">
		<id name="tranxCode" type="string">
		    <column name="TRANX_CODE" sql-type ="varchar2(2)" not-null="true" />
			<generator class="assigned" />
		</id>
	
		<property name="description" type="string">
			<column name="DESCRIPTION" sql-type="varchar2(40)" not-null="false" />
		</property>

	</class>

</hibernate-mapping>